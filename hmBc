4.vissza arouterbe
interface gigabitEthernet 0/0.10
encapsulation dot1Q 10
no sh
 do wr
exit
ipv6 unicast-routing

interface gigabitEthernet 0/0.20
encapsulation dot1Q 20
no sh
do wr
exit

interface gigabitEthernet 0/0.30
encapsulation dot1Q 30
no sh 
do wr
exit

gigabitEthernet 0/0.10
ipv6 address 2001:2023::10:1/64
ipv6 address FE80::10:1 LINK-local

interface gigabitEthernet 0/0.10
no sh
exit
interface gigabitEthernet 0/0.20
ip address 172.20.0.2 255.255.192.0
no sh
do wr
exit
interface gigabitEthernet 0/0.30
ip address 172.30.0.2 255.255.192.0
no sh
do wr
exit

vlan database
vlan 10 name ADMIN
vlan 20 name FAST
vlan 30 name SLOW
exit
do wr

5. vissza a switchbe 166-Sbe
interface range fasatEthernet 0/1-9
switchport mode access vlan 20
no sh
do wr
exit

interface range fastEthernet 0/10-18
switchport mode acces vlan 30
no sh
do wr
exit

interface range gigabitEthernet 0/1-2
switchport mode trunk
switchport mode trunk allowed vlan 10,20,30
switchport trunk native vlan 1
no sh
do wr
exit


6. vissza routerbe Guileta -Rbe
interface gigabitEthernet 0/0.30
standby version 2
standby ip 172.30.0.3
standby ? ne mfontos
standby priority 110
no sh
do wr
exit

configba
ip route 0.0.0.0 0.0.0. serial 0/0/1
do wr

router ospf 10
network 172.20.0.0 0.0.63.255 area 0
network 172.30.0.0 0.0.63.255 area 0
network 10.2.0.0 0.0.7 area area 0
do wr

passive-interface gigabitEthernet 0/0.20
passive-interface gigabitEthernet 0/0.30
do wr
default-information originate
do wr

interface gigabitEthernet 0/0.30
ip nat inside
interface s0/0/0
ip nat outside
interface s0/0/1
ip nat outside
exit
do wr
ip nat ? nem fontos
ip nat inside source static 172.30.30.1 10.2.0.6
acces-list 30 deny host 172.20.30.1 
do wr
access-list 30 permit  172.30.0.0 0.0.63.255
ip nat inside source list 30 interface serial 0/0/1 overload
do wr
